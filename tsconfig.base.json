{
    "compilerOptions": {
        "lib": ["ES2022", "DOM"],
        "target": "ES2022",
        "module": "esnext",
        "moduleResolution": "bundler",
        "skipDefaultLibCheck": true,
        "skipLibCheck": true,
        "experimentalDecorators": true,
        "forceConsistentCasingInFileNames": true,
        "noFallthroughCasesInSwitch": true,
        "noImplicitOverride": true,
        "noImplicitReturns": true,
        "strict": true
    }
}
